{
  "name": "my-better-t-app",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "dotenv": "^17.2.2",
      "zod": "^4.1.13",
      "typescript": "^5",
      "next": "^16.1.1",
      "@trpc/server": "^11.7.2",
      "@trpc/client": "^11.7.2"
    }
  },
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "format": "prettier --write . '!(node_modules|.next|.turbo|dist|build)/**/*'"
